utils::citHeader("To cite package 'inldata' in publications use:")

authors <- c(
  utils::person(
    given = c("Jason", "C."),
    family = "Fisher"
  ),
  utils::person(
    given = c("Allison", "R."),
    family = "Trcka"
  ),
  utils::person(
    given = c("Kerri", "C."),
    family = "Treinen"
  )
)

utils::citEntry(
  entry = "TechReport",
  author = authors,
  title = "inldata---Collection of datasets for the U.S. Geological Survey - Idaho National Laboratory groundwater and surface-water monitoring networks, v1.1",
  address = "Reston, Va.",
  institution = "U.S. Geological Survey",
  series = "U.S. Geological Survey software release",
  year = "2024",
  note = "R package",
  version = "1.1",
  doi = "10.5066/P9IAKQOR",
  textVersion = "Fisher, J.C., Trcka, A.R., Treinen, K.C., 2024, inldata---Collection of datasets for the U.S. Geological Survey - Idaho National Laboratory groundwater and surface-water monitoring networks, v1.1: U.S. Geological Survey software release, R package, https://doi.org/10.5066/P9IAKQOR."
)

utils::citEntry(
  entry = "TechReport",
  author = authors,
  title = "Datasets for the U.S. Geological Survey - Idaho National Laboratory groundwater and surface-water monitoring networks, v1.1",
  address = "Reston, Va.",
  institution = "U.S. Geological Survey",
  series = "U.S. Geological Survey data release",
  year = "2024",
  version = "1.1",
  doi = "10.5066/P9UWRYR4",
  textVersion = "Fisher, J.C., Trcka, A.R., Treinen, K.C., 2024, Datasets for the U.S. Geological Survey - Idaho National Laboratory groundwater and surface-water monitoring networks, v1.1: U.S. Geological Survey data release, https://doi.org/10.5066/P9UWRYR4."
)
